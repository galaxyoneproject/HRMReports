  <DataSets>
    <DataSet Name="dsActualContracts">
      <Query>
        <DataSourceName>srcHRM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmployeeID">
            <Value>=Parameters!EmployeeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        e.ID as EmployeeID, e.FullName, c.ContractText
FROM            Employees AS e 
LEFT OUTER JOIN
                  (SELECT        EmployeeID, MAX(ID) AS CurrentContractID
                   FROM            Contracts
	WHERE OrderState = 1
                   GROUP BY EmployeeID
) AS cc ON e.ID = cc.EmployeeID 

LEFT OUTER JOIN
                         Contracts AS c ON c.ID = cc.CurrentContractID
WHERE        (e.ID = ISNULL(@EmployeeID, e.ID))</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeId">
          <DataField>EmployeeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ContractText">
          <DataField>ContractText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
