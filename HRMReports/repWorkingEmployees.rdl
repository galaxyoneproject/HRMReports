  <DataSets>
    <DataSet Name="dsWorkingEmployees">
      <Query>
        <DataSourceName>srcHRM</DataSourceName>
        <CommandText>SELECT        e.*, p.Name as PostName, d.Name as DepartmentName
FROM            Employees AS e 
INNER JOIN
                  (SELECT        EmployeeID, MAX(ID) AS CurrentContractID
                   FROM            Contracts
	WHERE OrderState = 1
                   GROUP BY EmployeeID
) AS cc ON e.ID = cc.EmployeeID
LEFT JOIN    
	dbo.Contracts c on c.ID = cc.CurrentContractID
LEFT JOIN    
	dbo.Posts p ON c.PostID = p.ID 
LEFT JOIN
                  dbo.Departments d ON c.DepartmentID = d.ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
